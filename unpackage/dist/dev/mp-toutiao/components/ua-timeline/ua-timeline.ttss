/* Timeline
	--------------------------------*/
.ve__timeline.data-v-29d93229 {
  font-size: 14px;
  margin: 0;
  padding: 0;
  list-style: none;
}
.ve__timeline-item.data-v-29d93229 {
  padding-bottom: 20px;
  position: relative;
}
.ve__timeline-item.data-v-29d93229:last-child {
  padding-bottom: 0;
}
.ve__timeline-item__line.data-v-29d93229 {
  border-left: 2px solid #e4e7ed;
  height: 100%;
  position: absolute;
  left: 5px;
}
.ve__timeline-item:last-child .ve__timeline-item__line.data-v-29d93229 {
  display: none;
}
.ve__timeline-item__node.data-v-29d93229 {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  background-color: #e4e7ed;
  border-radius: 50%;
  height: 12px;
  width: 12px;
  position: absolute;
}
.ve__timeline-item__node [class*=ve-icon-].data-v-29d93229 {
  color: #fff;
  font-size: inherit;
}
.ve__timeline-item__dot.data-v-29d93229 {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  position: absolute;
}
.ve__timeline-item__body.data-v-29d93229 {
  padding-left: 30px;
  position: relative;
  top: -3px;
}
.ve__timeline-item__content.data-v-29d93229 {
  color: #303133;
  font-size: inherit;
  font-family: system-ui;
  word-break: break-all;
}
.ve__timeline-item__timestamp.data-v-29d93229 {
  color: #909399;
  font-size: 12px;
  line-height: 1;
}
.ve__timeline-item__timestamp.is-top.data-v-29d93229 {
  margin-bottom: 7px;
  padding-top: 3px;
}
.ve__timeline-item__timestamp.is-bottom.data-v-29d93229 {
  margin-top: 7px;
}

/*type*/
.ve__timeline-item--primary .ve__timeline-item__node.data-v-29d93229 {
  background-color: #409eff;
}
.ve__timeline-item--primary .ve__timeline-item__dot.data-v-29d93229 {
  color: #409eff;
}
.ve__timeline-item--success .ve__timeline-item__node.data-v-29d93229 {
  background-color: #31c48d;
}
.ve__timeline-item--success .ve__timeline-item__dot.data-v-29d93229 {
  color: #31c48d;
}
.ve__timeline-item--warning .ve__timeline-item__node.data-v-29d93229 {
  background-color: #e6a23c;
}
.ve__timeline-item--warning .ve__timeline-item__dot.data-v-29d93229 {
  color: #e6a23c;
}
.ve__timeline-item--danger .ve__timeline-item__node.data-v-29d93229 {
  background-color: #f56c6c;
}
.ve__timeline-item--danger .ve__timeline-item__dot.data-v-29d93229 {
  color: #f56c6c;
}
.ve__timeline-item--info .ve__timeline-item__node.data-v-29d93229 {
  background-color: #909399;
}
.ve__timeline-item--info .ve__timeline-item__dot.data-v-29d93229 {
  color: #909399;
}

/*卡片card*/
.ve__timeline--card .ve__timeline-item__content.data-v-29d93229 {
  border-radius: 4px;
  box-shadow: 0 1px 4px #dcdfe6;
  padding: 20px;
}